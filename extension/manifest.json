{
  "manifest_version": 3,
  "name": "Algo-Vault Collector",
  "version": "1.0",
  "description": "Collects code submissions from LeetCode and CodeChef.",
  "permissions": [
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "https://leetcode.com/*",
    "https://www.codechef.com/*",
    "http://localhost:5000/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
